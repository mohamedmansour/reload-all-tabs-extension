{
  "name": "__MSG_extName__",
  "version": "6.2.0",
  "manifest_version": 3,
  "description": "__MSG_extDescription__",
  "default_locale": "en",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "action": {
    "default_icon": "img/icon128.png",
    "default_title": "__MSG_extActionTitle__"
  },
  "background": {
    "service_worker": "reload_controller.js"
  },
  "options_page": "options.html",
  "permissions": [
    "storage",
    "tabs",
    "contextMenus",
    "tabGroups"
  ],
  "commands": {
    "reload": {
      "suggested_key": {
        "default": "Ctrl+Shift+R",
        "mac": "Command+Shift+R"
      },
      "description": "__MSG_commandToggleReload__"
    }
  }
}